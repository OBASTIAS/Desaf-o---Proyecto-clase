<!DOCTYPE html>
<html>
  <head>
  
    <title>SecondApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
	   
  </head>
	
  	<body class="no-sidebar">
  		
		<!-- Header -->
		<div id="header">
			<nav id="nav">
				<ul>
					<li><%= link_to "Index", pages_index_path %></li>
					<li><%= link_to "Test", pages_test_path %></li>
					<li><%= link_to "Login", users_login_path %></li>
					<li><%= link_to "About Us", pages_about_us_path %></li>
				</ul>
			</nav>
		</div>
			
    
    	<!-- Main -->
    	<div class="wrapper">
			<div class="container">
				<%= yield%>
				
		</div>
  
    	<!-- Footer -->
		<div id="footer">
			<div class="container">
				<div class="row">
					<div class="col-12">
			      		<div class="copyright">
					    	<ul class="menu">
						    	<li>&copy; Untitled. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li><li>Demo images: Michael Domaradzki</li>
					    	</ul>
						</div>
			  		</div>
				</div>
			</div>
		</div>
	</body>
	<%= javascript_include_tag 'application' %>	 
  

</html>

